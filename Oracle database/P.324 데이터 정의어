#Q1

CREATE TABLE EMP_HW(
    EMPNO NUMBER(4),
    ENAME VARCHAR2(10),
    JOB VARCHAR2(9),
    MGR NUMBER(4),
    HIREDATE DATE,
    SAL NUMBER(7,2),
    COMM NUMBER(7,2),
    DEPTNO NUMBER(2)
    );
    
    
#Q2

ALTER TABLE EMP_HW
        ADD BIGO VARCHAR2(20)

#Q3

ALTER TABLE EMP_HW
        MODIFY BIGO VARCHAR(20)

#Q4

ALTER TABLE EMP_HW
        RENAME COLUMN BIGO TO REMARK

#Q5

INSERT INTO EMP_HW
    SELECT EMPNO,ENAME,JOB,MGR,HIREDATE,SAL,COMM,DEPTNO,NULL
        FROM EMP

#Q6

DROP TABLE EMP_HW