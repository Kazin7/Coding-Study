n,m = map(int , input().split())

bowl = []

for i in range(n):
  bowl.append(list(map(int , input().split())))

def dfs(x,y):
  if(x >= n or x < 0 or y < 0 or y >= m):
    return False
  if(bowl[x][y] == 0):
    bowl[x][y] = 1

    dfs(x,y+1)
    dfs(x,y-1)
    dfs(x+1,y)
    dfs(x-1,y)
    return True
  return False

count = 0;
for i in range(n):
  for j in range(m):
    if(dfs(i,j) == True):
      count += 1

print(count)
